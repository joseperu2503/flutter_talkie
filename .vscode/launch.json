    "version": "0.2.0",
    "configurations": [
      {
        "name": "flutter_talkie",
        "request": "launch",
        "type": "dart",
        "program": "lib/main.dart",
        "args": [
          "-d", 
          "chrome",
          "--web-browser-flag=--disable-web-security"
        ]
      },
      {
        "name": "corfid-extranet-web (profile mode)",
        "request": "launch",
        "type": "dart",
        "flutterMode": "profile"
      },
      {
        "name": "corfid-extranet-web (release mode)",
        "request": "launch",
        "type": "dart",
        "flutterMode": "release"
      }
    ]
  }